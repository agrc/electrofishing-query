{
  "name": "@ugrc/electrofishing-query",
  "version": "2.0.3",
  "private": true,
  "homepage": "https://github.com/agrc/electrofishing-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/electrofishing-query.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "firebase emulators:start --import emulator_data --export-on-exit --only functions,auth,ui,firestore",
    "dev:vite": "vite",
    "format": "prettier . --write",
    "postinstall": "cd functions && npm install",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently 'wait-on http://127.0.0.1:4000 && npm run dev:vite' 'npm run dev:firebase-emulators'",
    "start:ci": "vite --host",
    "storybook": "storybook dev -p 6006",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.31.4",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.60.5",
    "@tanstack/react-table": "^8.20.5",
    "@ugrc/layer-selector": "^6.2.9",
    "@ugrc/tailwind-preset": "^1.1.0",
    "@ugrc/utah-design-system": "^1.15.1",
    "firebase": "^11.0.2",
    "immer": "^10.1.1",
    "ky": "^1.7.2",
    "lodash.debounce": "^4.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "use-immer": "^0.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/react": "^8.4.4",
    "@storybook/react-vite": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/eslint__js": "^8.42.3",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-storybook": "^0.11.0",
    "globals": "^15.12.0",
    "happy-dom": "^15.11.6",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "storybook": "^8.4.4",
    "tailwindcss": "^3.4.15",
    "tailwindcss-react-aria-components": "^1.1.6",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^2.1.5",
    "wait-on": "^8.0.1"
  }
}
