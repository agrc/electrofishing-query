{
  "name": "@ugrc/electrofishing-query",
  "version": "2.0.0-4",
  "homepage": "https://github.com/agrc/electrofishing-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/electrofishing-query.git"
  },
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev": "npm start",
    "dev:vite": "vite",
    "dev:firebase-emulators": "firebase emulators:start --import emulator_data --export-on-exit --only functions,auth,ui,firestore",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "check": "tsc -b",
    "preview": "vite preview",
    "start": "run-p dev:*",
    "start:ci": "vite --host",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.30.9",
    "@heroicons/react": "^2.1.5",
    "@tanstack/react-query": "^5.56.2",
    "@ugrc/layer-selector": "^6.2.8",
    "@ugrc/utah-design-system": "^1.9.4",
    "firebase": "^10.13.2",
    "immer": "^10.1.1",
    "ky": "^1.7.2",
    "react": "^18.3.1",
    "react-aria": "^3.34.3",
    "react-aria-components": "^1.3.3",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-stately": "^3.32.2",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1",
    "use-immer": "^0.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tanstack/eslint-plugin-query": "^5.57.2",
    "@types/eslint__js": "^8.42.3",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@ugrc/tailwind-preset": "^1.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.2",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "tailwindcss-react-aria-components": "^1.1.5",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.7",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^2.1.1"
  }
}
