{
  "name": "@ugrc/electrofishing-query",
  "version": "2.0.8",
  "private": true,
  "homepage": "https://github.com/agrc/electrofishing-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/electrofishing-query.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "firebase emulators:start --import emulator_data --export-on-exit --only functions,auth,ui,firestore",
    "dev:vite": "vite",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase-emulators'",
    "start:ci": "vite --host",
    "storybook": "storybook dev -p 6006",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.32.10",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.76.1",
    "@tanstack/react-table": "^8.21.3",
    "@ugrc/layer-selector": "^6.4.0",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/utah-design-system": "^1.27.1",
    "@ugrc/utilities": "^3.0.4",
    "firebase": "^11.8.0",
    "immer": "^10.1.1",
    "ky": "^1.8.1",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-aria": "^3.40.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-stately": "^3.38.0",
    "tailwind-merge": "^2.6.0",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@storybook/addon-links": "^9.0.18",
    "@storybook/react-vite": "^9.0.18",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@ugrc/eslint-config": "^1.2.2",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.27.0",
    "firebase-tools": "^13.35.1",
    "globals": "^15.14.0",
    "happy-dom": "^17.4.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.14",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^9.0.18",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.1.4",
    "wait-on": "^8.0.3",
    "@storybook/addon-docs": "^9.0.18"
  }
}
