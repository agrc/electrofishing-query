{
  "name": "@ugrc/electrofishing-query",
  "version": "2.0.7",
  "private": true,
  "homepage": "https://github.com/agrc/electrofishing-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/electrofishing-query.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "firebase emulators:start --import emulator_data --export-on-exit --only functions,auth,ui,firestore",
    "dev:vite": "vite",
    "format": "prettier . --write",
    "postinstall": "cd functions && npm install",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently 'wait-on http://127.0.0.1:4000 && npm run dev:vite' 'npm run dev:firebase-emulators'",
    "start:ci": "vite --host",
    "storybook": "storybook dev -p 6006",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.32.9",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.71.1",
    "@tanstack/react-table": "^8.21.2",
    "@ugrc/layer-selector": "^6.3.0",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/utah-design-system": "^1.22.0",
    "firebase": "^11.6.0",
    "immer": "^10.1.1",
    "ky": "^1.8.0",
    "lodash.debounce": "^4.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^5.0.0",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.23.0",
    "@storybook/addon-essentials": "^8.6.11",
    "@storybook/addon-interactions": "^8.6.11",
    "@storybook/addon-links": "^8.6.11",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.6.11",
    "@storybook/react-vite": "^8.6.11",
    "@storybook/test": "^8.4.7",
    "@tanstack/eslint-plugin-query": "^5.68.0",
    "@types/eslint__js": "^9.14.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "cpy-cli": "^5.0.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.14.0",
    "happy-dom": "^16.5.3",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^8.4.7",
    "tailwindcss": "^3.4.17",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.4",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^3.1.1",
    "wait-on": "^8.0.3"
  }
}
