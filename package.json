{
  "name": "@ugrc/electrofishing-query",
  "version": "2.0.12",
  "private": true,
  "homepage": "https://github.com/agrc/electrofishing-query",
  "repository": {
    "type": "git",
    "url": "https://github.com/agrc/electrofishing-query.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "build-storybook": "storybook build",
    "check": "tsc -b",
    "copy:arcgis": "cpy \"./node_modules/@arcgis/core/assets/**\" ./public/assets",
    "dev:firebase-emulators": "./scripts/start-emulators.sh",
    "dev:vite": "vite",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently 'wait-on http://127.0.0.1:4000 && pnpm run dev:vite' 'pnpm run dev:firebase-emulators'",
    "start:ci": "vite --host",
    "storybook": "storybook dev -p 6006",
    "test": "vitest"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@arcgis/core": "^4.34.8",
    "@heroicons/react": "^2.2.0",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-table": "^8.21.3",
    "@ugrc/tailwind-preset": "^1.2.0",
    "@ugrc/utah-design-system": "2.0.0",
    "@ugrc/utilities": "^3.1.1",
    "firebase": "^12.9.0",
    "immer": "^11.0.1",
    "ky": "^1.14.3",
    "lodash.debounce": "^4.0.8",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-aria": "^3.46.0",
    "react-dom": "^19.2.4",
    "react-error-boundary": "^6.0.0",
    "react-stately": "^3.44.0",
    "tailwind-merge": "^2.6.0",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.3",
    "@ugrc/eslint-config": "^1.2.3",
    "@ugrc/tsconfigs": "^1.0.3",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cpy-cli": "^7.0.0",
    "eslint": "^9.38.0",
    "firebase-tools": "^15.6.0",
    "globals": "^17.3.0",
    "happy-dom": "^20.5.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "^10.2.8",
    "tailwindcss": "^3.4.18",
    "tailwindcss-react-aria-components": "^1.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-package-version": "^1.1.0",
    "vitest": "^4.0.18",
    "wait-on": "^9.0.3"
  }
}
